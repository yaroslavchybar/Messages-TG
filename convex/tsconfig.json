{
    "compilerOptions": {
        "target": "ESNext",
        "lib": [
            "ES2021"
        ],
        "module": "ESNext",
        "moduleResolution": "Bundler",
        "allowSyntheticDefaultImports": true,
        "strict": true,
        "skipLibCheck": true,
        "noEmit": true,
        "isolatedModules": true
    },
    "include": [
        "./**/*"
    ],
    "exclude": [
        "node_modules",
        "_generated"
    ]
}